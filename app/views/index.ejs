<!DOCTYPE html>
<html lang="en" ng-app="Animal_App">
  <head>
    <link rel="stylesheet" href="/public/css/css.css"/>
    <link rel="stylesheet" href="/node_modules/angular-material/angular-material.css">
    <meta name="viewport" content="initial-scale=1" />
  </head>
  <body ng-controller="AppCtrl"  layout="column">
    <md-toolbar layout="row">
      <div class="md-toolbar-tools">
        <md-button layout-align="center" ng-click="toggleSidenav('left')" hide-gt-sm class="md-icon-button">
          <img src="/public/imgs/ic_menu_white_36dp_1x.png">
        </md-button>
        <h1>Animal Classification App</h1>
      </div>
    </md-toolbar>
    <div layout="row" flex>
        <md-sidenav layout="column" class="md-sidenav-left md-whiteframe-z2" md-component-id="left" md-is-locked-open="$mdMedia('gt-sm')">
          <md-button class="md-raised md-accent md-whiteframe-0dp" href="/#!/">
            Home
          </md-button>
          <md-button class="md-raised md-accent md-whiteframe-0dp" href="/#!/animals">
            View all Animals
          </md-button>
          <md-button class="md-raised md-accent md-whiteframe-0dp" href="/#!/animals/create">
            Create New Animal
          </md-button>
        </md-sidenav>
        <div layout="column" flex id="content">
            <md-content layout="column" flex class="md-padding md-whiteframe-2dp">


              <section ng-view></section>
              
              
            </md-content>
        </div>
    </div>
    <!-- Angular Material Dependencies -->
    <script src="/node_modules/angular/angular.js"></script>
    <script src="/node_modules/ng-file-upload/dist/ng-file-upload.min.js"></script>
    <script src="/node_modules/angular-route/angular-route.js"></script>
    <script src="/node_modules/angular-resource/angular-resource.js"></script>
    <script src="/node_modules/angular-animate/angular-animate.js"></script>
    <script src="/node_modules/angular-aria/angular-aria.js"></script>
    <script src="/node_modules/angular-material/angular-material.js"></script>
    
    <script src="/public/module.js"></script>
    
    <script src="/public/controllers/animals.public.controller.js"></script>
    
    <script src="/public/services/animals.public.service.js"></script>
    
    <script src="/public/routes/routing.public.route.js"></script>
    
    <script src="/public/js/js.js"></script>
  </body>
</html>